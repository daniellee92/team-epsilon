<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js" type="text/javascript"></script>
<br/>
<div class="row row-offcanvas row-offcanvas-left">

  	<div class="column col-md-1"></div>
    <div class="column col-md-6">
      <div class="panel" style="background-color: transparent;" align="center">
        <div class="panel-body">
          <% images = @feedback.images
          if images.blank? %>
          	<img src="data:image/png;base64," style="width: 500px; height: 430px; border: 1px solid black;"/>
          <% else %>

            <div id="carousel" class="carousel slide carousel-fade" data-ride="carousel" style="width: 400px; height: 400px;">
              
              <!-- Carousel Indicators -->
              <ol class="carousel-indicators middle">
                <% i = 0
                  images.each do |image| %>

                  <% if i == 0 %>
                    <li data-target="#carousel" data-slide-to=<%= i.to_s %> class="active"></li>
                  <% else %>  
                    <li data-target="#carousel" data-slide-to=<%= i.to_s %> ></li>
                  <% end %>

                <% i += 1
                  end %>                  
              </ol>

              <!-- Carousel items -->
              <div class="carousel-inner" align="center">
                <% i = 1
                  images.each do |image| %>
                    <div class="item <%= "active" if i == 1  %>">
                      <a data-target=<%="#image#{i.to_s}_modal"%> data-toggle="modal">
                        <img src= <%= "data:image/jpeg;base64," + image.image_data %> href=<%="#image#{i.to_s}_modal" %> style="width: 400px; height: 400px; border: 2px solid black;" class="rotate90"/>
                      </a>
                    </div>
                  <% i += 1
                  end %>
              </div>

              <div class="row">
                <% @feedback.images.each_with_index do |image, i| %>
                  <!--Popup Image -->
                  <div class="modal fade" id=<%="image#{(i+1).to_s}_modal"%> style="width: 500px; height: 500px; background-color: transparent; align: center;">
                  	
                  	<!-- Annotations -->
                  	<% s = 600 %>
                   	<figure class="annotated" style="width:<%= s %>px;height:<%= s %>px;">
                   		<img src= <%= "data:image/jpeg;base64," + image.image_data %> style="width: <%= s %>px; height: <%= s %>px; border: 3px sold black;" class="rotate90" />
                   		<figcaption>
						    <ul>
						      <% image.annotations.each do |annotation| %>
							    <!-- Positions of the list-items. These need defining inline. -->
							    <% x = annotation.x_axis * 600 / 320 - 25
							       y = annotation.y_axis * 600 / 320 - 25 %>
							  	<li style="top:<%= y.to_s%>px; left:<%= x.to_s %>px;" ><%= annotation.details %></li>
							  <% end %>
						    </ul>    
						</figcaption>
					</figure>

                  </div>
                <% end %>
              </div>

              <!-- Carousel nav -->
              <a class="carousel-control left" href="#carousel" role="button" data-slide="prev" style="text-decoration:none; right: 10px;">&lsaquo;</a>
              <a class="carousel-control right" href="#carousel" role="button" data-slide="next" style="text-decoration:none;">&rsaquo;</a>
              <!-- Carousel Picture Indicators -->

            </div>
          <% end %>
        </div>
      </div>
      
      <div class="panel panel-info">
        <div class="panel-heading">
          <div style="color: #3b5998;">
            <i class="icon-comment icon-2x" style="vertical-align: middle;"></i>
            <b style="font-size: 16px;"><%= pluralize(@feedback.comments.count, "Comment") %></b>

	        	<!-- Vote function (For vote and unvote) -->
	            <v style="float: right;">
	              <% 
	                enable_vote = true
	                if !current_user.blank?
	                  user_vote = @feedback.votes.where("user_id = ?", current_user.id).take
	                  if !user_vote.blank? 
	                    enable_vote = false 
	              %>
	                    <%= link_to raw('<i class="icon-bell-alt icon-2x" style="vertical-align: middle;"></i>'), feedbacks_unvote_path(:vote_id => user_vote.id, :feedback_id => @feedback.id), :method => :delete %>
	                  <% end %>
	                <% end %>

	              <% if enable_vote %>
	                <%= link_to raw('<i class="icon-bell icon-2x" style="vertical-align: middle;"></i>'), :action => 'create_vote', :feedback_id => @feedback.id %>
	              <% end %>

	              <b style="font-size: 16px;"><%= pluralize(@feedback.votes.count, "Vote") %></b>
	            </v>
          </div>
        </div>

        <!-- Comment function -->
        <div class="panel-body">
          
          <% if !current_user.blank? %>
            <div style="margin-top: 15px; margin-bottom: -10px">
              <%= form_tag url_for(:action => 'create_comment') do %> 
                <%= hidden_field_tag :feedback_id, @feedback.id %>
                
                <% if current_user.user_type == "User" %>
                  <b style="color: #3b5998;">
              	<% else %>
                  <b style="color: #B03060;">
              	<% end %>
                <%= current_user.nickname %></b>&nbsp;

                <%= text_field_tag :details, nil, placeholder: "Share your thoughts...", required: true, style: "height: 30px;" %>
              <% end %>
            </div>
          <% else %>
          	<div style="margin-top: 7px;">
            	<b><u><%= link_to "Sign in / Sign Up to comment", new_user_session_path%></u></b>
            </div>
          <% end %>

          <% @feedback.comments.reverse_order.each do |comment|
        	user = User.find(comment.user_id) %>
        	<div class="horizontal-line" style="background-color: #c4cde0;"></div>

            <% if user.user_type == "User" %>
              <b style="color: #3b5998;">
          	<% else %>
              <b style="color: #B03060;">
          	<% end %>
          	<%= user.nickname %></b>

          	<t style="font-size: 12px; color: grey;"><%= comment.created_at.strftime("%-d %b, %l:%M%P") %></t><br/>
          	<%= comment.details %>	
          <% end %>

        </div>
      </div>
    </div>

    <div class="column col-md-4">

      <!--Title-->
      <div style="font-size: 28px; font-weight: bold;"><%= "#" + @feedback.id.to_s + " - " + @feedback.title %></div>

      <!-- Reported Time, Location, Reported By -->
      <div style="color: grey; font-size: 14px;">
        <%= @feedback.created_at.strftime("%-d %b, %l:%M%P") %>  By <%= @nickname %><br/>
        <i class="icon-map-marker icon-2x" style="vertical-align: middle;"></i>  <%= @feedback.address %>
      </div>

      <br>

      <!-- Description -->
      <div style="font-size: 16px;"><%= @feedback.details %></div>

      <div class="horizontal-line" style="background-color: black;"></div> 

      <!-- Progress Related Display Start -->
      <div style="font-size: 16px; padding-top: 8px;">  
        <% 
          progress_status = @feedback.progress_status
          display_assign_progress_status = false

          if progress_status == "Marked As Resolved"
            user_verification_deadline = @feedback.datetime_marked_as_resolved + 3.days
            if Time.now <= user_verification_deadline
              progress_status = "Marked As Resolved (Pending User Verification)"
            end
          end 
        %>
        <!-- <div style="margin-bottom: 5px;"> -->
        	<b>Progress Status: </b><%= progress_status %>
    	<!-- </div> -->

        <% if !@feedback.handled_by.blank? %>
          <% agency = User.find(@feedback.handled_by) %>
          </br><b>Handled By: </b><%= agency.nickname %>
        <% end %>

        <% if !current_user.blank? %>
          <% if current_user == agency and progress_status == "In Progress" %>
            <%= button_to "Marked As Resolved", :controller => 'feedbacks', :action => 'marked_as_resolved', :feedback_id => @feedback.id %>

          <% elsif @feedback.progress_status == "Marked As Resolved" and current_user.id == @feedback.user_id %>
            <% display_assign_progress_status = true %>

          <% elsif current_user.user_type == "Admin" and progress_status == "Marked As Resolved" %>
            <% display_assign_progress_status = true %>
          <% end %>
        <% end %>

        <% if display_assign_progress_status %>
          <%= form_tag url_for(:controller => 'feedbacks', :action => 'assign_progress_status'), :class => 'form-search' do %> 
          <div class="input-group">
            <%= hidden_field_tag :feedback_id, @feedback.id %>
            <%= select_tag(:progress_status_new, options_for_select({ "Resolved" => "Resolved", "Unresolved" => "In Progress" }), prompt: "Resolved?", required: true, :onchange => "this.form.submit();", class: "form-control", :style => "width: 120%") %>
        </div>
          <% end %>
        <% end %>
      </div>
 

      <!-- *** Abuse Related Display Start *** -->
      <div style="font-size: 16px; margin-bottom: 5px;">
        <b>Abuse Status: </b><%= @feedback.abuse_status %>
        <% if @feedback.abuse_status == "Reported" and !@feedback.abuse_reason.blank? %>
        	<br/><b>Abuse Reason: </b><%= @feedback.abuse_reason %>
        <% end %>
      </div>

	  <div style="padding-left: 11px; padding-right: 11px;">
        <% if !current_user.blank? and current_user.user_type == "Admin" %>
	        <div class="row">
	        	<%= form_tag url_for(:controller => 'admin', :action => 'assign_abuse_status'), :class => 'form-search' do %> 
		          	<div class="input-group">
			            <%= hidden_field_tag :feedback_id, @feedback.id %>
			            <%= select_tag(:abuse_status_new, options_for_select({ "Abusive" => "Abusive", "Not Abusive" => "Not Abusive" }), prompt: "Abuse?", required: true, :onchange => "this.form.submit();", class: "form-control", :style => "width: 120%") %>
		      			<!-- <span class="input-group-btn">
		      				<button type="submit" class="btn btn-search" style="color: white; background: #3b5998;">Set Abuse Status</button>
		            	</span> -->
		            </div>
		        <% end %>
	      	</div>

        <% else %>
		    <!-- (General) User, Agency: Enable/ Prevent report abuse-->
		    <% if @feedback.abuse_status == "Nil" %>
		        <div class="row">
			          <!-- Redirect user to sign in page if not signed in -->
				    <%= form_tag url_for(:controller => 'feedbacks', :action => 'report_abuse'), :class => 'form-search' do %>
				        <div class="input-group">

					        <%= hidden_field_tag :feedback_id, @feedback.id %>
						    <span class="input-group-btn">
				      			<button type="submit" href="#report" class="btn btn-search" style="color: white; background: #800517; margin-left: 0.1cm;" data-toggle="modal">Report Abuse</button>
				            </span>

				            <!--Popup Abuse -->
				            <div class="modal fade" id="report">
				                <div class="modal-content">
				                	<div class="modal-body">
						                <%= text_area_tag :abuse_reason, nil, placeholder: "Share your reasons...", required: true, rows: 3, class: "form-control" %>
						            </div>
						            <div class="modal-footer">
						                <button type="submit" class="btn btn-search" style="color: white; background: #900000;">Report Abuse</button>
						            </div>
					            </div>
				            </div>
				        </div>
				    <% end %>
			   	</div>
		    <% end %>
        <% end %>
      </div>
      <!-- *** Abuse Related Display End *** -->

      <!-- start of google map -->
      <div align="center">
        <div id="map" style='width: 360px; height: 250px' align="center"></div>
        <script style="text/javascript">
          handler = Gmaps.build('Google');
          handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
            markers = handler.addMarkers(<%=raw @hash.to_json %>);
            handler.bounds.extendWith(markers);
            handler.fitMapToBounds();
            handler.getMap().setZoom(16);
          });
        </script>
      </div>
      <!-- end of google map -->

    </div>
    <div class="column col-md-1"></div>
  </div>
